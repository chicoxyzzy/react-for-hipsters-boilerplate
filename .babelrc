{
  "stage": 2,
  "optional": ["es7.classProperties"],
  "loose": "all",
  "plugins": [
    "react-transform"
  ],
  "extra": {
    "react-transform": [
      {
        "target": "react-transform-webpack-hmr",
        "imports": ["react"],
        "locals": ["module"]
      }, {
        "target": "react-transform-catch-errors",
        "imports": ["react", "redbox-react"]
      }
    ]
  }
}